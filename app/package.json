  "private": true,
  "dependencies": {
    "@aws-amplify/ui-react": "^6.1.9",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.18",
    "@watergis/mapbox-gl-legend": "^1.1.2",
    "aws-amplify": "^6.2.0",
    "jquery": "^3.7.1",
    "maplibre-gl": "^4.2.0",
